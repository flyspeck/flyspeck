	      let e03max = arcmax cs (p3,p4) +. arcmax cs (p4, p0) in
		e03min <= e03max in
      let bunfinished = not (exists (transfer_to cs) terminals) in
	bstr && bunfinished && forall sph_tri_ineq (ks_cs cs);;


