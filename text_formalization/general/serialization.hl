let digest_thms some_thml = 
  let digests =  map 
    (fun t -> try full_digest_thm t;
     with 
       | Failure s -> (report ("cannot digest: "^(string_of_thm t)); "") 
       | Not_found -> (report ("cannot digest, not found: "^(string_of_thm t)); "")) 
    some_thml in
    filter (fun s -> not (s = "")) digests;;
  
