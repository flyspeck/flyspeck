let des_make_thm = 
  let n = ref 0 in 
    fun p t ->
      try
	let th = mk_ser_thm t in
	let _ = (n := !n + 1) in
	let _ = not(0 = !n mod 20) or (report "(!:)";true) in 
	  th
      with Failure s -> (report_not_found t ;
			 let thm = p t in
			   add_thm thm; thm);;

